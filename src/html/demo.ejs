<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
  <style>
    body {
      margin: 0;
    }
    table {
      border-collapse: collapse;
      width: 600px;
      margin: 0px auto 30px;
    }
  </style>
</head>
<body>
  <% studentList.forEach(function (student) { %>
    <table border="1">
      <colgroup>
        <col width="100"/>
        <col/>
      </colgroup>
      <tr>
        <th>お名前</th>
        <td><%= student.name %></td>
      </tr>
      <tr>
        <th>年齢</th>
        <td><%= student.age %></td>
      </tr>
      <tr>
        <th>住所</th>
        <td><%= student.address %></td>
      </tr>
      <tr>
        <th>趣味</th>
        <td>
          <% if (student.likes.length) { %>
            <div>
              <% student.likes.forEach(function (like) { %>
                <div><%= like %></div>
              <% }) %>
            </div>
          <% } else { %>
            <div>趣味はありません</div>
          <% } %>
        </td>
      </tr>
    </table>
  <% }) %>
</body>
</html>